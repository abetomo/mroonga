DROP TABLE IF EXISTS items;
CREATE TABLE items (
id INT PRIMARY KEY AUTO_INCREMENT NOT NULL,
prefix INT,
value BIGINT,
INDEX (prefix, value desc)
) DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
